<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="question/type/comparetexttask/db" VERSION="20120122" COMMENT="XMLDB file for Moodle question/type/comparetexttask"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="question_comparetexttask" COMMENT="Info about a CompareTextTask question">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" NEXT="questionid"/>
        <FIELD NAME="questionid" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false" COMMENT="Foreign key references question.id." PREVIOUS="id" NEXT="correctorfeedback"/>
        <FIELD NAME="correctorfeedback" TYPE="text" NOTNULL="true" SEQUENCE="false" COMMENT="Feedback for the corrector." PREVIOUS="questionid" NEXT="initialtext"/>
        <FIELD NAME="initialtext" TYPE="text" NOTNULL="true" SEQUENCE="false" COMMENT="Initial Text defined by the Professor." PREVIOUS="correctorfeedback" NEXT="avaiabletags"/>
        <FIELD NAME="avaiabletags" TYPE="text" NOTNULL="true" SEQUENCE="false" COMMENT="Avaiable Tags defined by the Professor." PREVIOUS="initialtext" NEXT="sample"/>
        <FIELD NAME="sample" TYPE="text" NOTNULL="true" SEQUENCE="false" COMMENT="Sample Solution." PREVIOUS="avaiabletags"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" NEXT="questionid"/>
        <KEY NAME="questionid" TYPE="foreign" FIELDS="questionid" REFTABLE="question" REFFIELDS="id" PREVIOUS="primary"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>
